{
	"name": "zombieland",
	"type": "module",
	"version": "0.0.0",
	"description": "Expressen client testing framework",
	"main": "zombieland.js",
	"license": "BSD-3-Clause",
	"exports": {
		"import": "./zombieland.js",
		"require": "./zombieland.cjs"
	},
	"scripts": {
		"test": "mocha 'packages/**/*-test.js' 'examples/**/test.js'",
		"lint": "eslint .",
		"prepack": "esbuild zombieland.js --bundle --platform=node --packages=external --format=cjs --outdir=. --out-extension:.js=.cjs --define:import.meta.filename=__filename"
	},
	"engines": {
		"node": ">=20"
	},
	"keywords": [
		"headless",
		"browser",
		"fake",
		"mock",
		"IntersectionObserver",
		"MutationObserver",
		"iframe"
	],
	"author": "Bonnier News",
	"repository": {
		"type": "git",
		"url": "git://github.com/BonnierNews/zombieland.git"
	},
	"dependencies": {
		"@zombieland/little-rock": "file:packages/little-rock",
		"@zombieland/tallahassee": "file:packages/tallahassee",
		"@zombieland/wichita": "file:packages/wichita",
		"jsdom": "^23.0.1",
		"nock": "^14.0.0-beta.13",
		"specificity": "^1.0.0",
		"whatwg-fetch": "^3.6.20"
	},
	"devDependencies": {
		"@eslint/js": "^9.11.1",
		"chai": "^5.1.1",
		"esbuild": "^0.24.0",
		"eslint": "^9.11.0",
		"globals": "^15.9.0",
		"lodash.pick": "^3.1.0",
		"mocha": "^10.7.3",
		"mocha-cakes-2": "^3.3.0"
	},
	"bugs": {
		"url": "https://github.com/BonnierNews/zombieland/issues"
	},
	"homepage": "https://github.com/BonnierNews/zombieland#readme"
}
